{
  "name": "logni-cookie.js",
  "version": "0.1.2",
  "description": "Cookie javascript library for work with cookies (supporting LogNI logger)",
  "main": "src/logni-cookie.js",
  "module": "build/js/logni-cookie-es5.js",
  "directories": {
    "test": "test",
    "build": "build"
  },
  "scripts": {
    "test": "jshint --verbose --show-non-errors src/logni-cookie.js",
    "build": "mkdir -p build/js && ./node_modules/babel-minify/bin/minify.js src/logni-cookie.js -o build/js/logni-cookie.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikni/logni-cookie.js.git"
  },
  "keywords": [
    "logni",
    "logging",
    "cookie",
    "cookies",
    "javascript",
    "js"
  ],
  "author": "Erik Brozek <erik+logni@brozek.name>",
  "license": "GPL3 <https://github.com/erikni/logni.js/blob/master/LICENSE>",
  "bugs": {
    "url": "https://github.com/erikni/logni-cookie.js/issues"
  },
  "homepage": "https://github.com/erikni/logni-cookie.js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "npx": "^10.2.0",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "braces": ">=2.3.1"
  }
}
